{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"]
    }
  },
  "include": [
    "index.d.ts",
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "app/subpage/apple-body.jsx",
    "lib/hooks/useLax.jsx",
    "lib/lax.js",
    "lib/laxPresets.js",
    "app/(main)/(components)/shadered-moo.jsx"
, "app/(main)/(components)/main-background.jsx"  ],
  "exclude": ["node_modules"]
}
